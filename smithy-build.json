{
    "version": "1.0",
    "sources": ["models"],
    "maven": {
      "dependencies": [
        "software.amazon.smithy.typescript:smithy-typescript-codegen:0.18.0"
      ]
    },
    "plugins": {
      "typescript-client-codegen": {
        "package": "@smithy/typescript-example-client",
        "packageVersion": "0.0.1"
      },
      "typescript-server-codegen": {
        "package": "@smithy/typescript-example-server",
        "packageVersion": "0.0.1",
        "disableDefaultValidation": true
      }
    }
  }
  